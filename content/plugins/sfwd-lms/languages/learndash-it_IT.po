"\t\t\t\t\t\t\t\t\t\t\t\t\t\t<li>pass</li>\n"
"\t\t\t\t\t\t\t\t\t\t\t\t\t\t<li>rank</li>\n"
"\t\t\t\t\t\t\t\t\t\t\t\t\t\t<li>timestamp</li>\n"
"\t\t\t\t\t\t\t\t\t\t\t\t\t\t<li>points*</li>\n"
"\t\t\t\t\t\t\t\t\t\t\t\t\t\t<li>total_points*</li>\n"
"\t\t\t\t\t\t\t\t\t\t\t\t\t\t<li>percentage</li>\n"
"\t\t\t\t\t\t\t\t\t\t\t\t\t\t<li>timespent*</li>\n"
"\t\t\t\t\t\t\t\t\t\t\t\t\t</ol>\n"
